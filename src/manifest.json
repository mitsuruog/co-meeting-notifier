{
	"name": "co-meeting Notifier",
	"description": "Displays your co-meeting unread counts",
	"version": "0.1.1",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon-19.png"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"tabs",
		"https://www.co-meeting.com/api/v1/*"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}


